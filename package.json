{
  "name": "tradezzz",
  "version": "1.0.0",
  "description": "AI-Powered Crypto Trading Platform - Multi-user research & execution with GOAP, SAFLA, and AgentDB",
  "main": "dist/index.js",
  "bin": {
    "neural-trading": "./bin/cli.js"
  },
  "scripts": {
    "start": "tsx src/api/server.new.ts",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:ui\"",
    "dev:ui": "vite --config vite.config.ts",
    "dev:api": "tsx watch src/api/server.new.ts",
    "dev:old": "tsx watch src/api/server.ts",
    "build": "tsc && npm run copy-assets",
    "build:ui": "vite build --config vite.config.ts",
    "copy-assets": "cp -r src/templates dist/ && cp -r config dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "swarm:plan": "npx claude-flow swarm 'create implementation plan' --strategy research --output plans/swarm-plan.md",
    "swarm:build": "npx claude-flow swarm 'build trading system' --strategy development --mode hierarchical",
    "swarm:test": "npx claude-flow swarm 'test trading system' --strategy testing --mode star",
    "install:deps": "npm install && npm run setup",
    "setup": "node scripts/setup.js",
    "lean-agentic": "npx lean-agentic",
    "midstreamer": "npx midstreamer"
  },
  "keywords": [
    "trading",
    "ai",
    "neural-networks",
    "agentdb",
    "goap",
    "safla",
    "algorithmic-trading",
    "swarm-intelligence",
    "machine-learning"
  ],
  "author": "rUv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/agentdb-site.git",
    "directory": "neural-trading"
  },
  "dependencies": {
    "@clerk/backend": "^2.27.0",
    "@clerk/clerk-react": "^5.58.1",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "agentdb": "^1.6.1",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inquirer": "^9.2.12",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.548.0",
    "ora": "^7.0.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^13.0.0",
    "ws": "^8.14.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "claude-flow": ">=2.0.0"
  },
  "optionalDependencies": {
    "lean-agentic": "^0.3.2",
    "midstreamer": "^0.2.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist",
    "bin",
    "config",
    "docs",
    "plans",
    "examples",
    "README.md",
    "LICENSE"
  ]
}
